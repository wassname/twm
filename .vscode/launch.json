    "version": "0.2.0",
    "configurations": [
        {
            "name": "test",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "autoReload": {"enable": true,},
            "env": {"WANDB_MODE":"disabled","WANDB_SILENT":"true", "TQDM_MININTERVAL": "5"},
            "args": [
                "--config=test",
                // "--device=cpu",
            ]
        },
        {
            "name": "run",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "autoReload": {"enable": true,},
            "env": {"TQDM_MININTERVAL": "30"},
            "args": [
                "--wandb=online",
                "--save",
            ]
        }
    ]
}
